{
    "module_id": "mattmacf:viam-app-skeleton",
    "visibility": "public",
    "url": "https://github.com/mattmacf98/viam-app-skeleton",
    "description": "Example Viam app skeleton with Svelte and Tailwind",
    "models": [
        {
            "api": "rdk:component:generic",
            "model": "mattmacf:viam-app-skeleton:viam-app-skeleton"
        }
    ],
    "first_run": "",
    "entrypoint": "./bin/viam-app-skeleton",
    "build": {
        "build": "make module",
        "setup": "make setup-linux",
        "arch": [
            "linux/amd64",
            "linux/arm64"
        ],
        "path" : "module.tar.gz"
    },
    "applications": [
      {
        "name": "viam-app-skeleton",
        "type": "single_machine",
        "entrypoint": "dist/index.html"
      }
    ]
  }